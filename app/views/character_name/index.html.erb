<div class="jumbotron" id="name-jumbotron">
  <h1><i class="glyphicon glyphicon-user"></i></h1>
  <h3><%= t('character_name.title') %></h3>
  <p><%= random(FirstName) %> <%= random(LastName) %></p>

  <%= link_to 'Randomize!', character_name_index_path, class: 'btn btn-warning' %>
</div>

<blockquote class="quote">
  <p class="text-muted"><%= t('character_name.quote') %></p>
</blockquote>

<p class="did_you_know"><%= t('character_name.example') %></p>
